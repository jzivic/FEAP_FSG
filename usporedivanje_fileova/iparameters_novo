   zf=0.484       ! kapa_op - intima                   (0.484)
   zg=0.487       ! kapa_op - media                    (0.487)
   zh=0.466       ! kapa_op - adventitia               (0.466)
   zi=20          ! sigmoid function curvature parameter for in-plane         (  20)
   zj=60          ! sigmoid function curvature parameter for out-of-plane     (  60)
   zk=0.64        ! parameter for kapa_ip change due to mass change           (0.64)
   zl=1           ! parameter for kapa_op change due to mass change           (   1)

PARAmeter
   zm=0.237       ! end kapa_ip - one layered wall model    (0.237)
   zn=0.479       ! end kapa_op - one layered wall model    (0.479)
   zo=0.261       ! end kapa_ip - intima                    (0.261)
   zp=0.208       ! end kapa_ip - media                     (0.208)
   zq=0.232       ! end kapa_ip - adventitia                (0.232)
   zr=0.484       ! end kapa_op - intima                    (0.484)
   zs=0.487       ! end kapa_op - media                     (0.487)
   zt=0.466       ! end kapa_op - adventitia                (0.466)
   zu=11d0        ! radius at which kapa starts to change
   zv=36.5d0      ! radius at which kapa changes no more

!--------------------------------------------------------------------------------------------------------------------------------

PARAmeter   ! -- I L T   n u m e r ic a l   m e t h o d   p a r a m e t e r s -- !
   d8=100         ! Maximal number of ILT element in radial direction
   d9=0.2         ! ILT element thickness
   d0=0.2       ! Required distance from wall to lumen surface to exit loop and add ILT elements
   da=-0.0        ! Delta_ilt_thickness - change of lumen surface radius in each modfeap simulation
   db=2           ! ILT elements material number
   dc=8           ! Pressure elements material number
   dd=10         ! Luminal surface radius
   de=120         ! z_min for luminal surface option [mm]
   df=130         ! z_max for luminal surface option [mm]
   dg=0           ! theta_min for luminal surface option [°]
   dh=90          ! theta_max for luminal surface option [°]
   di=9           ! material number for zero pressure
   dj=3           ! Ruptured ILT element material number
   dk=10          ! End radius od luminal surface [mm]
   dl=0.5         ! Luminal surface radius decrese distance [mm]
   dm=3           ! Number of timesteps between luminal surface radius decreases
   dn=0.4         ! TAWSS boundary value

PARAmeter   ! -- I L T   T o n g   m a t e r i a l   m o d e l -- !
   i1=0.0082      ! luminal mu parameter [MPa]        (0.0065 - 0.0082 - 0.0099)
   i2=0.0123      ! luminal k1 parameter [MPa]        (0.0086 - 0.0123 - 0.0160)
   i3=0.6         ! luminal k2 parameter              (0.3 - 0.6 - 0.9)
   i4=1.30        ! luminal ILT prestretch
   i5=0.0051      ! abluminal mu parameter [MPa]      (0.0040 - 0.0051 - 0.0062)
   i6=0.0029      ! abluminal k1 parameter [MPa]      (0.0019 - 0.0029 - 0.0039)
   i7=0.03        ! abluminal k2 parameter            (0.02 - 0.03 - 0.04)
   i8=1.00        ! abluminal ILT prestretch
   i9=2.0         ! depth of luminal layer [mm]
   i0=0.2         ! function curvature parameter
   ia=1        ! factor for reducing parameters of ruptured ILT elements
   ib=0.02       ! rupture stress [MPa]

PARAmeter   ! -- I L T   b i o c h e m o   m o d e l -- !
   j1=2.75        !-(?) w^e_q,elas - elastin weighting factor in degradation function [(g_elas*day)ˇ1]     e = elastin, elas = elastase
   j2=1.85        !-(?) w^k_q,MMP - collagen weighting factor in degradation function [(g_elas*day)ˇ1]
   j3=0.12        !-(?) K^elas_N - correlation factor  [g_elas/g_N]
   j4=5.5         !-(?) K^MMP_N - correlation factor  [g_MMP/g_N]
   j5=2d-6        !-(?) M^N_i - number of neutrofils in one layer
   j6=0.03        !-(?) layer thickness for M^N_i
   j7=0.00035     !-(?) K^elas_VV - correlation factor [g_elas/mm^2_VV]
   j8=0.00015     !-(?) K^MMP_VV - correlation factor [g_MMP/mm^2_VV]
   j9=5d-4        !-(?) A^VV_0 - vasa vasorum initial homeostatic surface
   j0=150d0       !-(?) K^VV_EDP - correlation factor [mm^2_VV/g_M^EDP_tot]  150
   ja=0.8         !-(?) ratio of EDP in total degraded elastin
   jb=1.060d-3	!-(?) rho - wall density, [g/mm^3]

